// 8.11(c)
#include <stdio.h>
#include <stdlib.h>

int find(int);
int main()
{
    find(12);
    return 0;
}

int find(int n)
{
    int i = 0, j = 1;
    while (1)
    {
        if ((j - 2) % 3 == 0)
            if ((j - 3) % 5 == 0)
                if ((j - 2) % 7 == 0)
                {
                    i++;
                    printf("%d\n", j);
                    if (i >= n)
                        return 0;
                }
        j++;
    }
}