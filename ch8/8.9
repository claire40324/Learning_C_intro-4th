#include <stdio.h>
#include <stdlib.h>

int prime(int num)
{
    int i;
    for (i = 2; i <= num - 1; i++)
    {
        if (num % i == 0)
        {
            return 0;
        }
    }
    if (num == 1)
        return 0;
    return 1;
}
int main(void)
{
    int sum = 0, i;
    for (i = 1; i <= 30; i++)
    {
        if (prime(i))
        {
            printf("%d ", i);
        }
    }
    return 0;
}