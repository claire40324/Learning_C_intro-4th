#include <stdio.h>
#include <stdlib.h>

void counter(void);
int fib2(int);

int main(void)
{
    fib2(5);
    return 0;
}
void counter(void)
{
    static int a = 1;
    printf("counter已被呼叫%d次了\n", a);
    a++;
}

int fib2(int n) 
{
    counter();
    if (n == 1 || n == 2)
    {
        return 1;
    }
    else
    {
        return (fib2(n - 1) + fib2(n - 2));
    }
}