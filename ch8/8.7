#include <stdio.h>
#include <stdlib.h>
int prime(int num)
{
    int i;
    for (i = 2; i <= num - 1; i++)
    {
        if (num % i == 0)
        {
            return 0;
        }
    }
    return 1;
}
int main(void)
{
    int i = 2, sum = 0;
    while (sum < 100)
    {
        if (prime(i))
        {
            printf("%d ", i);
            sum++;
        }
        i++; //不使用break的寫法
    }
    return 0;
}