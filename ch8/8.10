#include <stdio.h>
#include <stdlib.h>

int find_m_prime(int);
int main(void)
{
    int i = 1, sum = 0;
    while (sum < 8)
    {
        if (find_m_prime(i))
        {
            printf("%d\n", i);
            sum++;
        }
        i++;
    }
    return 0;
}
int find_m_prime(int num)
{
    int i, j, num1 = num;
    int mod;
    num++;
    for (i = 0; i <= num1; i++)
    {
        mod = num % 2;
        num = num / 2;
        if (mod != 0)
            return 0;
        if (num == 1)
            if (mod == 0)
                return 1;
            else
                return 0;
    }
}