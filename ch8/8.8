#include <stdio.h>
#include <stdlib.h>

double f1(double);
int main()
{
    printf("%1f\n", f1(-3.2));
    printf("%1f\n", f1(-2.1));
    printf("%1f\n", f1(0));
    printf("%1f\n", f1(2.1));
    return 0;
}

double f1(double x)
{
    int i, power = 1;
    double sum = 0;
    for (i = 1; i <= 3; i++)
    {
        power *= x;
    }
    sum = (power * 3) + (2 * x) - 1;
    return sum;
}