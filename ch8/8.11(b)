// 8.11(b)  443, 653
#include <stdio.h>
#include <stdlib.h>

int find(int);
int main()
{
    printf("%d, %d", find(5), find(7));
    return 0;
}

int find(int n)
{
    int i = 0, number, j = 1;
    while (1)
    {
        if ((j - 2) % 3 == 0)
            if ((j - 3) % 5 == 0)
                if ((j - 2) % 7 == 0)
                {
                    i++;
                    if (i >= n)
                        return j;
                }
        j++;
    }
}